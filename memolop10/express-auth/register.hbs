<form id="register-form" onsubmit="return submitRegister(event);">
    <div>
        <label for="name">Nombre: </label>
        <input type="text" id="name" name="name">
    </div>
    <div>
        <label for="email">Email: </label>
        <input type="text" id="email" name="email">
    </div>
    
    <div>
       <label for="password">password: </label>
        <input type="text" id="password" name="password">
    </div>

    <div>
       <label for="confirmarpassword">Confirmar password: </label>
        <input type="text" id="confirmarpassword" name="confirmarpassword">
    </div>
    <button type="submit">Agregar</button>
</form>
<script src="/js/submit-register.js"></script>